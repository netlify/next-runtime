{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2019",
      "dom"
    ],
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}