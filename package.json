{
  "name": "@netlify/plugin-nextjs",
  "version": "1.0.2",
  "description": "run Nextj.s seamlessly on netlify",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "build": "next build test/sample",
    "test": "npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/netlify-plugin-nextjs.git"
  },
  "keywords": [
    "nextjs",
    "netlify",
    "next",
    "netlify-plugin"
  ],
  "author": "lindsaylevine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/netlify-plugin-nextjs/issues"
  },
  "homepage": "https://github.com/netlify/netlify-plugin-nextjs#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "find-up": "^4.1.0",
    "make-dir": "^3.1.0",
    "next": "^9.5.3",
    "next-on-netlify": "^2.6.3",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@netlify/eslint-config-node": "^0.3.0",
    "cpy": "^8.1.1",
    "execa": "^4.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "path-exists": "^4.0.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tmp-promise": "^3.0.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check ."
    }
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js",
      "!**/test/fixtures/**",
      "!**/test/sample/**"
    ],
    "verbose": true
  }
}
